<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GCloudPhone.Views.WebViews.AGBsWebView"
             xmlns:mct="clr-namespace:CommunityToolkit.Maui.Behaviors;assembly=CommunityToolkit.Maui"
             xmlns:templates="clr-namespace:GCloudPhone.Views.Templates"
             Title="" NavigationPage.HasNavigationBar="False" Shell.NavBarIsVisible="False">
    <ContentPage.Behaviors>
        <mct:StatusBarBehavior StatusBarColor="White" />
    </ContentPage.Behaviors>
    <Grid 
        Style="{StaticResource BasicGrid}"
        RowDefinitions="Auto,*">
       
 
        <templates:BackButtonControl Grid.Row="0"/>

        <Grid Grid.Row="1">
            <ScrollView>
                <WebView x:Name="webView"  />
            </ScrollView>
        </Grid>

    </Grid>
</ContentPage>