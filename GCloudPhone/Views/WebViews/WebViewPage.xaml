<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:templates="clr-namespace:GCloudPhone.Views.Templates"
             x:Class="GCloudPhone.Views.WebViews.WebViewPage"
             Shell.NavBarIsVisible="False" NavigationPage.HasNavigationBar="False">
    <Grid  Style="{StaticResource BasicGrid}">
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"></RowDefinition>
            <RowDefinition Height="95*"></RowDefinition>
        </Grid.RowDefinitions>
        
        
        <templates:BackButtonControl/>
        
        <WebView x:Name="MyWebView"
                 Grid.Row="1"
                 
                 Navigated="MyWebView_Navigated"/>
    </Grid>
</ContentPage>