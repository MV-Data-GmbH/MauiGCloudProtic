<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GCloudPhone.Views.WebViews.JobsWebView"
             xmlns:mct="clr-namespace:CommunityToolkit.Maui.Behaviors;assembly=CommunityToolkit.Maui"
             xmlns:templates="clr-namespace:GCloudPhone.Views.Templates"
             Title="" NavigationPage.HasNavigationBar="False" Shell.NavBarIsVisible="False">
    <ContentPage.Behaviors>
        <mct:StatusBarBehavior StatusBarColor="{StaticResource Zuta}" />
    </ContentPage.Behaviors>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"></RowDefinition>
            <RowDefinition Height="95*"></RowDefinition>
        </Grid.RowDefinitions>

        <templates:BackButtonControl/>

        <WebView Grid.Row="1" x:Name="webView" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand" />

    </Grid>
</ContentPage>